{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "dist/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "test:coverage": {
      "outputs": [
        "coverage/**"
      ]
    },
    "e2e": {
      "outputs": []
    },
    "type-check": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:migrate": {
      "outputs": []
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    }
  }
}
